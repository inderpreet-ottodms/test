{
  "name": "mfg.com",
  "version": "8.5.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node ./generate-version.js && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve --configuration=dev",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=prod",
    "build": "node ./generate-version.js && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod  --aot=true --extract-css=false --output-hashing=all",
    "build-linux-image": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=linux  --aot=true --extract-css=false --output-hashing=all",
    "build-prod-patch": "npm version patch --no-git-tag-version && node ./replace.build.js && npm run build && node ./versioning.build.js",
    "build-prod-no-patch": "node ./replace.build.js && npm run build && node ./versioning.build.js",
    "build:qa": "node ./generate-version.js && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=qa && npm run sentry:sourcemaps",
    "build:uat": "node ./generate-version.js && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=uat  --aot=true --output-hashing=all && npm run sentry:sourcemaps",
    "build:dev": "node ./generate-version.js && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=dev",
    "build:production": "node ./generate-version.js && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production  --aot=true --output-hashing=all && npm run sentry:sourcemaps",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org otto-dms --project javascript-angular ./dist && sentry-cli sourcemaps upload --org otto-dms --project javascript-angular ./dist"
  },
  "config": {
    "max_old_space_size": 8192
  },
  "private": true,
